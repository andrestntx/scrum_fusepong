<%= render "layouts/developer/navbar" %>

<div id="page-wrapper">
  <div class="row">
      <div class="col-lg-12">
          <h1 class="page-header">
            <%= link_to 'Users /', users_developer_path, class: 'breadcrumb-text' %> 
            <%= @user.name %>
          </h1>
          
      </div>
      <!-- /.col-lg-12 -->
  </div>
  <!-- /.row -->
  <div class="row">
    <!-- User info -->
    <%= render "users/info" %>
    <!-- User projects  -->
    <%= render "users/developer_projects" %>
    <!-- User dailies  -->
    <div class="col-xs-12">
      <h2 class="h4">Dailies:</h2>
      <%= render "users/dailies" %>
    </div>
  </div>
  <!-- /.row -->
</div>
<!-- /#page-wrapper -->