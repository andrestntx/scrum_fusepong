<div class="col-xs-12 col-sm-7 col-md-8">
  <h2 class="h4">Dailies:</h2>
  <!-- Project Sprint Table  -->
  <div class="row">
    <div class="col-xs-12">
      <table class="table table-hover">
        <thead>
          <tr>
            <th>Date</th>
            <th>Time</th>
            <th>User</th>
            <th>Comments</th>
          </tr>
        </thead>

        <tbody>
          <% @sprint.dailies.each do |daily| %>
            <tr>
              <td><%= daily.created_at.strftime("%Y-%m-%d") %></td>
              <td><%= daily.daily_time.name %></td>
              <td><%= daily.user.name %></td>
              <td><%= daily.comments %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <!-- /.col-xs-12 -->
  </div>
  <!-- /.row -->
</div>