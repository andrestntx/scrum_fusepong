<%= render "layouts/developer/navbar" %>

<div id="page-wrapper">
  <div class="row">
      <div class="col-lg-12">
          <h1 class="page-header">
          	<%= link_to 'Projects /', projects_developer_path, class: 'breadcrumb-text' %> 
            <%= link_to "#{ @sprint.project.name } / ", project_developer_path(@sprint.project), class: 'breadcrumb-text' %> 
            Sprint <%= @sprint.number %>
          </h1>
          
      </div>
      <!-- /.col-lg-12 -->
  </div>
  <!-- /.row -->
  <div class="row">
    <!-- Project info -->
    <div class="col-xs-12 col-sm-5">
      <p id="notice"><%= notice %></p>
    	<p>
    	  <strong>Description:</strong>
    	  <%= @project.description %>
    	</p>
      <p>
        <strong>Client:</strong>
        <%= @project.client %>
      </p>
      <p>
        <strong>Started at:</strong>
        <%= @project.started_at %>
      </p>
    </div>
  </div>
  <!-- /.row -->
</div>
<!-- /#page-wrapper -->